function runTime (x){
    currTime = 50; 
    var timeout = 5000;
    

    for(i=10; i > 1; i--);{

        setTimeout(function(){
        x.innerHTML = currTime
        currTime = currTime - 5; 
        }, timeout);
        timeout = timeout + 5000;
    
    if(1 > i);{
        setTimeout(function(){
        x.innerHTML = "Blastoff"
        }, timeout);
    }
}
}
