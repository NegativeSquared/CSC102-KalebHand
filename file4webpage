function getUserInput(){
    do{

            carmake = prompt("car make: ");
            carmodel= prompt("car Model: ");

            car = carmake + " " + carmodel;

       }   while(car.length > 15);

}

var IntervalID = new Array();

function runTime(x){
    
    var currTime = 50; 
    var timeout = f000;
        
            for(i=0;i<11;i++){
                

        IntervalID[i] = setTimeout(function(){
            if( currTime == 0){
                x.innerHTML = currTime
                alert("Blastoff");
            }
            else if( currTime < 25 ){
                x.innerHTML = "Warning Less Than Half Way To Launch, Time Left = " + currTime

            }
            else {
                x.innerHTML = currTime
            }
            currTime = currTime - 5;
        },timeout);
        timeout = timeout + 1000;

    }
    
}



